AntiAFK.Name = "AntiAFK"
AntiAFK.Parent = Frame
AntiAFK.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
AntiAFK.BorderColor3 = Color3.fromRGB(94, 0, 0)
AntiAFK.BorderSizePixel = 5
AntiAFK.Position = UDim2.new(0.0216932595, 0, 0.504878044, 0)
AntiAFK.Size = UDim2.new(0, 430, 0, 50)
AntiAFK.Font = Enum.Font.SourceSans
AntiAFK.Text = "Remove Sheild"
AntiAFK.TextColor3 = Color3.fromRGB(212, 1, 1)
AntiAFK.TextSize = 21.000
AntiAFK.MouseButton1Down:connect(function()
	_G.Toggle = true
	while _G.Toggle do
		wait()
		for i, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
			if v.Name == "ForceField" then
				v:Destroy()
			end
		end
	end
end)

