-- Scripts:

local function PZJIGWV_fake_script() -- Forward.Script 
	local script = Instance.new('Script', Forward)

	print("Hello world!")
	
end
coroutine.wrap(PZJIGWV_fake_script)()
local function UXIZ_fake_script() -- Back.Script 
	local script = Instance.new('Script', Back)

	print("Hello world!")
	
end
coroutine.wrap(UXIZ_fake_script)()
local function QVVX_fake_script() -- TextButton.Script 
	local script = Instance.new('Script', TextButton)

	print("Hello world!")
	
end
coroutine.wrap(QVVX_fake_script)()
